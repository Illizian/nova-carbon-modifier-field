{
    "name": "illizian/nova-carbon-modifier-field",
    "description": "A field for allowing a User to configure a \"Carbon Modifier\"",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Illizian\\NovaCarbonModifier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illizian\\NovaCarbonModifier\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
